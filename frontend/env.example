# Frontend Environment Variables
# Copy this to .env and fill in your values
# NEVER commit .env to git!

# Backend URL
# For local development:
VITE_BACKEND_URL=http://localhost:4000
# For production (Render):
# VITE_BACKEND_URL=https://your-backend-name.onrender.com

# Smart Contract Addresses (Sepolia Testnet)
VITE_TTK_ADDRESS=0x0000000000000000000000000000000000000000
VITE_ESCROW_ADDRESS=0x0000000000000000000000000000000000000000

# WalletConnect Project ID
# Get from: https://cloud.walletconnect.com/
VITE_WC_PROJECT_ID=your_walletconnect_project_id_here

# Sepolia RPC URL
# Get from: https://infura.io/ or https://www.alchemy.com/
VITE_SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_key_here



